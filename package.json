{
    "name": "manywho-ui-bootstrap",
    "version": "2.65.0",
    "description": "ManyWho HTML5 React Bootstrap components",
    "author": "ManyWho",
    "repository": {
        "type": "git",
        "url": "https://github.com/manywho/ui-bootstrap.git"
    },
    "scripts": {
        "check-types": "tsc",
        "docs": "typedoc js --out docs --ignoreCompilerErrors --mode modules & start docs/index.html",
        "start": "webpack --config webpack.development.config.js --env.build=../ui-html5/build --env.assets=local --env.watch --env.sourcemaps",
        "dev": "webpack --config webpack.development.config.js",
        "dist": "webpack -p --config webpack.production.config.js",
        "precommit": "lint-staged",
        "test": "jest",
        "lint": "eslint js/**"
    },
    "lint-staged": {
        "*.ts": "eslint",
        "*.tsx": [
            "eslint",
            "git add",
            "jest --findRelatedTests --bail"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.4.0",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
        "@babel/preset-typescript": "^7.3.3",
        "@types/enzyme": "^3.1.10",
        "@types/jest": "^21.1.10",
        "@types/lodash": "^4.14.123",
        "@types/react": "16.3.2",
        "@typescript-eslint/eslint-plugin": "^1.5.0",
        "babel-jest": "^21.2.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "chart.js": "^2.7.2",
        "clean-webpack-plugin": "^0.1.19",
        "compression-webpack-plugin": "1.1.12",
        "css-loader": "^0.28.11",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-14": "^1.0.5",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^5.15.3",
        "eslint-config-airbnb-typescript": "^1.1.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.12.4",
        "file-loader": "^1.1.11",
        "husky": "^0.14.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "22.4.3",
        "jest-junit-reporter": "^1.1.0",
        "jquery": "^2.1.4",
        "jquery-textcomplete": "^1.8.4",
        "less": "^2.7.3",
        "less-loader": "^4.1.0",
        "license-webpack-plugin": "^1.3.1",
        "lint-staged": "^5.0.0",
        "react": "^16.3.2",
        "react-addons-test-utils": "^0.14.8",
        "react-collapse": "^4.0.3",
        "react-dom": "^16.3.2",
        "react-dom-factories": "^1.0.0",
        "react-dropzone": "^4.2.10",
        "react-maskedinput": "^4.0.1",
        "react-motion": "^0.5.2",
        "react-selectize": "^3.0.1",
        "react-test-renderer": "^16.3.2",
        "react-transition-group": "^1.0.0",
        "script-loader": "^0.7.2",
        "string-replace-loader": "^2.1.1",
        "style-loader": "^0.19.1",
        "ts-jest": "^21.2.4",
        "typedoc": "^0.9.0",
        "typescript": "^3.3.4000",
        "webpack": "4.26.1",
        "webpack-bundle-analyzer": "^2.11.1",
        "webpack-cli": "3.1.2",
        "write-file": "1.0.0"
    },
    "jest": {
        "collectCoverage": true,
        "collectCoverageFrom": [
            "**/js/components/*.{ts,tsx}"
        ],
        "setupFiles": [
            "<rootDir>/test-setup.js"
        ],
        "setupTestFrameworkScriptFile": "<rootDir>/setup-tests-globals.js",
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json"
        ],
        "moduleNameMapper": {
            "\\.(css|less)$": "identity-obj-proxy"
        },
        "testResultsProcessor": "./node_modules/jest-junit-reporter"
    },
    "dependencies": {
        "@babel/plugin-transform-react-jsx": "^7.3.0",
        "@types/react-maskedinput": "^4.0.3",
        "babel-loader": "^8.0.5",
        "create-react-class": "^15.6.3",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "lint": "^1.1.2",
        "mini-css-extract-plugin": "^0.5.0",
        "ramda": "^0.25.0"
    }
}
