{
    "extends": "tslint-config-airbnb",
    "linterOptions": {
        "exclude": [
            "js/services/metadata.ts"
        ]
    },
    "rules": {
        "indent": [
            true,
            "spaces",
            4
        ],
        "max-line-length": [true, 150],
        "ter-indent": [true, 4],
        "variable-name": [true, "check-format", "allow-pascal-case"]
    }
}