{
    "extends": "tslint-config-airbnb",
    "rules": {
        "indent": [
            true,
            "spaces",
            4
        ],
        "max-line-length": [true, 150],
        "ter-indent": [true, 4],
        "variable-name": [true, "check-format", "allow-pascal-case"],
        "curly": false,
        "import-name": false,
        "no-boolean-literal-compare": false
    }
}