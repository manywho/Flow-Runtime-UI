{
    "compilerOptions": {
        "pretty": true,
        "outDir": "./runtime_build",
        "sourceMap": true,
        "allowJs": true,
        "jsx": "react",
        "lib": ["DOM", "ES6"],
        "moduleResolution": "node",
        "noUnusedLocals": true,
        // Must be false or unset for tests to pass
        "esModuleInterop": false
    },
    "include": ["ui-offline/js/**/*", "ui-bootstrap/js/**/*"]
}
