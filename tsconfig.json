{
  "exclude": [
    "node_modules",
    "./**/*.js",
    "./**/*.jsx"
  ],
  "files": [
    "custom.d.ts"
  ],
  "compilerOptions": {
    "pretty": true,
    "target": "es5",
    "allowJs": true,
    "sourceMap": true,
    "jsx": "react",
    "lib": ["dom", "es6", "es2015"],
    "moduleResolution": "node",
    // "noUnusedLocals": true,
    "module": "es6",
    "allowSyntheticDefaultImports": true
  }
}
